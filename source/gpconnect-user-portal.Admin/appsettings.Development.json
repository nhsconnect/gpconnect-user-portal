{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Sso": {
    "Flow": "code",
    "AuthScheme": "Cookies",
    "ChallengeScheme": "OpenIdConnect",
    "RequireHttpsMetadata": false,
    "AuthEndpoint": "http://auth.docker.internal:8079/default/authorize",
    "TokenEndpoint": "http://auth.docker.internal:8079/default/token",
    "MetadataEndpoint": "http://auth.docker.internal:8079/default/.well-known/openid-configuration",
    "ClientId": "debug-client",
    "ClientSecret": "not-really-very-secret",
    "CallbackPath": "/auth/externallogin",
    "SignedOutCallbackPath": "/auth/externallogout"
  }
}
